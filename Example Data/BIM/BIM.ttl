                ex:t_Receivables_collateral a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Receivables%20collateral%20are%20assets%20in%20the%20form%20of%20rights%20to%20payments%20related%20to%20debts%2C%20unsettled%20transactions%20or%20other%20monetary%20obligations%20owed%20to%20a%20company%20by%20its%20debtors%20or%20customers.%20The%20following%20types%20of%20collateral%20are%20considered%20receivables%20within%20the%20Bank%2C%20as%20far%20as%20they%20comply%20with%20the%20applicable%20eligibility%2C%20valuation%20and%20monitoring%20criteria%3A%3Cbr%3E%3F%20Trade%20receivables""";
                rdfs:label "Receivables collateral" .
                
                ex:t_Special_purpose_entity a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20special%20purpose%20entity%20is%20an%20entity%20that%20has%20been%20set-up%20to%20accomplish%20a%20narrow%20or%20well-defined%20objective.""";
