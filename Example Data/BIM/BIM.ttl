                rdfs:label "Safe deposit service" .
                
                ex:t_Transfer_to_withdrawal_from_reserves a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Transfer%20to%2Fwithdrawal%20from%20reserves%20is%20an%20equity%20flow%20amount%20and%20represents%20last%20year%D3%B3%20result%20that%20has%20been%20transferred%20to%20reserves%2C%20or%20for%20withdrawal%20from%20reserves.""";
                rdfs:label "Transfer to/withdrawal from reserves" .
                
                ex:t_Maturity_ladder_approach a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """The%20maturity%20ladder%20approach%20is%20a%20market%20risk%20approach%20that%20shows%20the%20net%20liquidity%20position%20horizon%20through%20the%20classification%20of%20maturities%20in%20maturity%20bands%20to%20which%20future%20cash%20flows%20must%20be%20assigned.%20To%20apply%20this%20approach%2C%20the%20bank%20shall%20use%20a%20separate%20maturity%20band%20for%20each%20commodity%20in%20line%20with%20CRR%20Article%20359.%20""";
                rdfs:label "Maturity ladder approach" .
                
                ex:t_International_bank_account_number a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """An%20International%20Bank%20Account%20Number%20(IBAN)%20is%20a%20unique%20number%20to%20identify%20bank%20accounts%20from%20around%20the%20world.%20""";
                rdfs:label "International bank account number" .
                
                ex:t_Retention_of_randomly_selected_exposures a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Retention%20of%20randomly%20selected%20exposures%20is%20a%20securitisation%20retention%20type%20in%20which%20no%20less%20than%205%25%20of%20the%20nominal%20value%20of%20randomly%20selected%20exposures%20is%20retained%2C%20where%20such%20exposures%20would%20otherwise%20have%20been%20securitised%20in%20the%20securitisation%2C%20provided%20that%20the%20number%20of%20potentially%20securitised%20exposures%20is%20no%20less%20than%20100%20at%20origination.""";
