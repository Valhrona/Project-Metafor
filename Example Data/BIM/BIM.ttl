                ex:t_SBI a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """SBI%20(standaard%20bedrijfsindeling)%20is%20a%20classification%20scheme%20from%20the%20Dutch%20Chamber%20of%20Commerce%20that%20categorises%20organisations%20based%20on%20what%20the%20organisation%20produces%2C%20trades%20in%2C%20or%20manufactures.""";
