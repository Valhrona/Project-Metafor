                ex:t_Blocked_account_indicator a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20blocked%20account%20indicator%20indicates%20whether%20an%20account%20is%20blocked%20by%20the%20Bank.""";
