                ex:t_Standard___Poor_s_credit_rating_grade a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """The%20Standard%20%26%20Poor's%20credit%20rating%20grade%20is%20the%20grade%20that%20expresses%20the%20Standard%20%26%20Poor's%20credit%20rating.""";
