                ex:t_Advance_collateral_value_rate a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """The%20advance%20collateral%20value%20rate%20is%20the%20percentage%20of%20the%20gross%20collateral%20value%20that%20The%20Bank%20is%20prepared%20to%20provide%20in%20advance.""";
