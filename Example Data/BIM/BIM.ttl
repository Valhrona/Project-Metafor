                ex:t_Foundation_internal_rating_based_approach a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """The%20foundation%20internal%20rating-based%20approach%20is%20an%20internal-rating%20based%20approach%20in%20which%20banks%20provide%20their%20own%20estimates%20of%20probability%20of%20default%20and%20rely%20on%20supervisory%20estimates%20for%20other%20risk%20components.""";
                rdfs:label "Foundation internal rating-based approach" .
                
                ex:t_Involved_party_type a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """An%20involved%20party%20type%20is%20a%20classification%20Scheme%20that%20distinguishes%20between%20involved%20parties%20according%20to%20their%20inherent%20characteristics%20and%20structure.""";
                rdfs:label "Involved party type" .
                
                ex:t_Credit_facility_arrangement_type a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20credit%20facility%20arrangement%20type%20distinguishes%20between%20credit%20facility%20arrangements.""";
                rdfs:label "Credit facility arrangement type" .
                
                ex:t_Guarantee_provided a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20guarantee%20provided%20is%20a%20guarantee%20provided%20by%20the%20bank.%3Cbr%3E""";
                rdfs:label "Guarantee provided" .
                
                ex:t_Cost_of_housing a dl:Data_Requirement, dl:BIM_Concept ;
