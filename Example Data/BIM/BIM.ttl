                ex:t_Risk_country a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20risk%20country%20is%20a%20country%20rated%20uniform%20counterparty%20rating%202%2B%20or%20weaker.""";
                rdfs:label "Risk country" .
                
                ex:t_Overnight_limit a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """An%20overnight%20limit%20is%20a%20drawn%20credit%20limit%20that%20has%20to%20be%20repaid%20the%20next%20day.""";
