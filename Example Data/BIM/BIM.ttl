                ex:t_Arrangement_Type a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """An%20arrangement%20type%20is%20a%20classification%20scheme%20that%20distinguishes%20between%20arrangements%20according%20to%20the%20nature%20of%20the%20arrangement.""";
                rdfs:label "Arrangement Type" .
                
                ex:t_Net_Present_Value a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """The%20Net%20Present%20Value%20(NPV)%20is%20the%20difference%20between%20the%20discounted%20future%20cash%20inflows%20and%20the%20discounted%20future%20cash%20outflows.%20The%20discount%20of%20the%20cash%20flow%20is%20based%20on%20the%20time%20value%20of%20money%20and%20reflected%20in%20the%20applied%20discount%20rate.""";
