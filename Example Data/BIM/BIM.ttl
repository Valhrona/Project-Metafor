                dl:definition """ECB%20main%20refinancing%20rate%20is%20a%20reference%20rate%20that%20is%20the%20interest%20rate%20which%20banks%20do%20have%20to%20pay%20when%20they%20borrow%20money%20from%20the%20ECB.""";
                rdfs:label "ECB main refinancing rate" .
                
                ex:t_Linear_amortisation a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Linear%20amortisation%20is%20an%20amortisation%20type%20in%20which%20a%20fixed%20amortisation%20amount%20is%20based%20on%20the%20original%20principal%20amount%20and%20the%20tenor%20of%20the%20loan.%20The%20amortisation%20has%20a%20fixed%20frequency.""";
                rdfs:label "Linear amortisation" .
                
                ex:t_Financing_product_flow_amount_indicator a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Financing%20product%20flow%20amount%20indicator%20is%20an%20indicator%20that%20classifies%20the%20financing%20product%20flow%20amount.""";
                rdfs:label "Financing product flow amount indicator" .
                
                ex:t_Interest_paid_on_another_account a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Interest%20paid%20on%20another%20account%20is%20a%20deposit%20flow%20amount%20and%20represents%20the%20interest%20paid%20on%20another%20account.""";
                rdfs:label "Interest paid on another account" .
                
                ex:t_Country_code a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20country%20code%20is%20an%20identifier%20that%20identifies%20the%20country%20of%20residence%20or%20the%20country%20of%20incorporation%20of%20the%20counterparty%20according%20to%20the%20ISO%20land%20code.""";
                rdfs:label "Country code" .
                
                ex:t_IBNI_provision a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """An%20Incurred%20But%20Not%20Identified%20(IBNI)%20provision%20is%20a%20loan%20loss%20allowance%20taken%20for%20exposures%20where%20the%20loss%20event%20has%20incurred%20but%20where%20provisioning%20has%20not%20yet%20taken%20place%20due%20to%20the%20fact%20the%20loss%20event%20has%20not%20been%20identified%20yet.%20This%20provision%20is%20therefore%20calculated%20over%20the%20entire%20non-impaired%20portfolio.""";
                rdfs:label "IBNI provision" .
                
                ex:t_Remuneration_risk a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Remuneration%20risk%20is%20the%20risk%20that%20remuneration%20is%20inappropriate.%3Cbr%3ENote%3A%3Cbr%3ERemuneration%20risk%20is%20an%20operational%20risk%20if%20it%20leads%20to%20losses%20(e.g.%20because%20it%20may%20lead%20to%20internal%20fraud%20or%20administrative%20mistakes)%2C%20a%20reputation%20risk%20if%20it%20leads%20to%20negative%20media%20attention%20(e.g.%20because%20the%20level%20of%20remuneration%20results%20in%20public%20scrutiny)%20and%20a%20business%20risk%20if%20it%20leads%20to%20not%20being%20able%20to%20execute%20the%20desired%20strategy%20(e.g.%20because%20the%20remuneration%20is%20not%20attractive%20enough%20to%20attract%20the%20capable%20people).%20""";
                rdfs:label "Remuneration risk" .
                
                ex:t_Sovereign_risk a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Sovereign%20risk%20is%20the%20risk%20that%20the%20government%20of%20a%20sovereign%20state%20may%20default%20on%20its%20financial%20obligations.""";
                rdfs:label "Sovereign risk" .
                
                ex:t_Lien_position a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20lien%20position%20indicates%20the%20rank%20of%20the%20priority%20rights%20on%20the%20proceeds%20of%20an%20asset.""";
                rdfs:label "Lien position" .
                
                ex:t_Organisation_economic_intent a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """An%20organisation%20economic%20intent%20is%20a%20classification%20scheme%20that%20distinguishes%20between%20organisations%20according%20to%20their%20intention%20regarding%20financial%20gain.""";
                rdfs:label "Organisation economic intent" .
                
                ex:t_Market_value a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Market%20value%20is%20the%20price%20that%20would%20be%20received%20when%20selling%20an%20asset%20or%20paid%20to%20transfer%20a%20liability%20in%20an%20orderly%20transaction%20between%20market%20participants%20at%20the%20measurement%20date.""";
                rdfs:label "Market value" .
                
                ex:t_Solvency_or_liquidity_guarantee a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20solvency%20or%20liquidity%20guarantee%20is%20a%20guarantee%20where%20the%20guarantor%20declares%20to%20a%20local%20regulator%20that%20it%20guarantees%20support%20for%20the%20group%20company%20of%20the%20Bank%2C%20enabling%20the%20group%20company%20of%20the%20Bank%20to%20continue%20to%20meet%20the%20local%20solvency%20or%20liquidity%20requirements.""";
                rdfs:label "Solvency or liquidity guarantee" .
                
                ex:t_Effective_arrangement a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """An%20effective%20arrangement%20is%20an%20arrangement%20which%20is%20currently%20active%20and%20operating%20under%20the%20specified%20terms%20and%20conditions.""";
                rdfs:label "Effective arrangement" .
                
                ex:t_Concentration_risk a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Concentration%20risk%20is%20the%20risk%20of%20having%20large%20exposures%2C%20as%20related%20to%20the%20total%20risk%20exposure%20of%20the%20bank%2C%20to%20a%20single%20or%20several%20highly%20correlated%20risk%20sources.""";
                rdfs:label "Concentration risk" .
                
                ex:t_Pillar_1_capital_requirement a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Pillar%201%20capital%20requirement%20is%20the%20absolute%20minimum%20amount%20of%20capital%20as%20defined%20in%20the%20%5CBasel%20III%20framework%20Pillar%201%20Capital%5C%22%20that%20is%20required%20to%20cover%20the%20three%20major%20risk%20types%20that%20a%20bank%20faces%3A%20credit%20risk""";
                rdfs:label "Pillar 1 capital requirement" .
                
                ex:t_Minority_interest a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Minority%20interest%20is%20equity%20in%20a%20subsidiary%20not%20attributable%2C%20directly%20or%20indirectly%2C%20to%20the%20Bank.""";
                rdfs:label "Minority interest" .
                
                ex:t_Several_liability a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20several%20liability%20is%20an%20arrangement%20participation%20type%20in%20which%20two%20or%20more%20parties%20are%20each%20liable%20for%20only%20their%20respective%20obligations.""";
                rdfs:label "Several liability" .
                
                ex:t_Forward_deposit a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20forward%20deposit%20is%20a%20%5Bderivative%5D%20in%20the%20form%20of%20an%20agreement%20between%20two%20%5Bcounterparties%5D%20regarding%20the%20value%20or%20level%20of%20a%20%5Bfinancial%20instrument%5D%20at%20a%20future%20date.%20Forward%20deposits%20are%20flexible%2C%20as%20they%20can%20be%20structured%20to%20mature%20on%20any%20date.%20Forward%20deposit's%20do%20not%20involve%20any%20transfer%20of%20principal.%20It%20is%20settled%20at%20maturity%20in%20cash%2C%20representing%20the%20%5Bprofit%20or%20loss%5D%20resulting%20from%20the%20difference%20in%20the%20agreed%20rate%20and%20the%20settlement%20rate%20at%20maturity.""";
                rdfs:label "Forward deposit" .
                
                ex:t_Natural_disaster a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20natural%20disaster%20is%20a%20disasters%20and%20public%20safety%20event%20that%20involves%20loss%20of%20or%20damage%20to%20assets%20and%2For%20people%20as%20a%20consequence%20of%20natural%20disasters%20(including%20pandemics).""";
                rdfs:label "Natural disaster" .
                
                ex:t_Credit_valuation_adjustment a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Credit%20valuation%20adjustment%20is%20the%20difference%20between%20the%20risk-free%20portfolio%20value%20and%20the%20true%20portfolio%20that%20takes%20into%20account%20the%20possibility%20of%20the%20counterparty's%20default.%20In%20other%20words%2C%20credit%20valuation%20adjustment%20is%20the%20market%20value%20of%20counterparty%20credit%20risk.""";
                rdfs:label "Credit valuation adjustment" .
                
                ex:t_Discontinued_operation a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20discontinued%20operation%20is%20a%20component%20of%20an%20organisation%20that%20either%20has%20been%20disposed%20of%2C%20or%20is%20classified%20as%20held%20for%20sale%2C%20and%3A%3Cbr%3E-%20Represents%20a%20separate%20major%20line%20of%20business%20or%20geographical%20area%20of%20operations""";
                rdfs:label "Discontinued operation" .
                
                ex:t_Asset_correlation_factor a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """The%20asset%20correlation%20factor%20reflects%20the%20impact%20from%20systemic%20risk%20on%20the%20asset%20value%20of%20an%20obligor%2C%20i.e.%20dependency%20from%20economic%20trends%2C%20e.g.%20in%20industry%20or%20business%20outlook.%20In%20Basel%20II%20systemic%20risk%20is%20modelled%20as%20a%20single%20factor%20(so%20called%20Asymptotic%20Single%20Risk%20Factor%20Model).%20Per%20asset%20class%20a%20regulatory%20correlation%20factor%20has%20been%20determined.""";
                rdfs:label "Asset correlation factor" .
                
                ex:t_Original_maturity a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """The%20original%20maturity%20is%20the%20period%20between%20the%20start%20date%20and%20the%20of%26nbsp""";
                rdfs:label "Original maturity" .
                
                ex:t_Option_style a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """The%20option%20style%20distinguishes%20between%20styles%20of%20options.""";
                rdfs:label "Option style" .
                
                ex:t_Use_of_organisation_restructuring_provision a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Use%20of%20organisation%20restructuring%20provision%20is%20a%20resource%20item%20flow%20amount%20that%20represents%20the%20use%20of%20the%20provision%20for%20an%20organisation%20restructuring%20within%20the%20Bank.%3Cbr%3E%3Cbr%3EOud%3A%3Cbr%3EOrganisation%20restructuring%20provision%20is%20a%20resource%20item%20flow%20amount%20that%20represents%20the%20provision%20for%20an%20organisation%20restructuring%20within%20the%20Bank.""";
                rdfs:label "Use of organisation restructuring provision" .
                
                ex:t_Parent_company_identifier a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Parent%20company%20identifier%20is%20a%20unique%20identification%20of%20parent%20company%20in%20accordance%20with%20the%20codification%20used%20by%20the%20ESCB%20Register%20of%20Institutions%20and%20Affiliates%20Database%20(RIAD).""";
                rdfs:label "Parent company identifier" .
                
                ex:t_Collective_allowance a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Collective%20allowance%20is%20a%20loan%20loss%20allowance%20for%20assets%20that%20are%20managed%20within%20a%20portfolio%2C%20being%20assets%20with%20similar%20credit%20risk%20characteristics%20and%20which%20are%20collectively%20assessed%20for%20impairment.""";
                rdfs:label "Collective allowance" .
                
                ex:t_Credit_exposure a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20credit%20exposure%20is%20the%20amount%20which%20is%20outstanding%20(drawn)%20under%20a%20facility.%20This%20includes%20e.g.%20loans%2C%20advances%20and%20debt%20securities""";
                rdfs:label "Credit exposure" .
                
                ex:t_Ship_collateral a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Ship%20collateral%20is%20physical%20collateral%20that%20is%20characterised%20by%20a%20ship.""";
                rdfs:label "Ship collateral" .
                
                ex:t_Lender_identifier a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Lender%20identifier%20is%20a%20unique%20identification%20of%20lenders%20in%20accordance%20with%20the%20codification%20used%20by%20the%20ESCB%20Register%20of%20Institutions%20and%20Affiliates%20Database%20(RIAD).""";
                rdfs:label "Lender identifier" .
                
                ex:t_Original_collateral_value a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Original%20collateral%20value%20is%20the%20value%20of%20the%20collateral%20at%20the%20arrangement%20start%20date.""";
                rdfs:label "Original collateral value" .
                
                ex:t_Customer_intake___documentation a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20customer%20intake%20%26%20documentation%20is%20an%20execution%2C%20delivery%20and%20process%20management%20event%20at%20the%20back%20office%20that%20is%20related%20to%20incorrect%20or%20incomplete%20customer%20records%2C%20documentation%2C%20account%20data%20and%20customer%20permissions.%20""";
                rdfs:label "Customer intake & documentation" .
                
                ex:t_Financial_difficulty a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Financial%20difficulty%20is%20a%20status%20that%20indicates%20whether%20a%20counterparty%20is%20(potentially)%20entering%20into%20a%20situation%20that%20it%20is%20not%20(longer)%20able%20to%20meet%20its%20current%20or%20future%20obligations%20towards%20the%20bank.""";
                rdfs:label "Financial difficulty" .
                
                ex:t_Disposal_group a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20disposal%20group%20is%20a%20group%20of%20assets%20to%20be%20disposed%20of%2C%20by%20sale%20or%20otherwise%2C%20together%20as%20a%20group%20in%20a%20single%20transaction%2C%20and%20liabilities%20directly%20associated%20with%20those%20assets%20that%20will%20be%20transferred%20in%20the%20transaction.""";
                rdfs:label "Disposal group" .
                
                ex:t_Idiosyncratic_market_risk a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """An%20idiosyncratic%20risk%20is%20a%20market%20risk%20that%20primarily%20captures%20changes%20in%20the%20credit%20worthiness%20of%20the%20issuer.""";
                rdfs:label "Idiosyncratic market risk" .
                
                ex:t_Bond a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20bond%20is%20debt%20instrument%20that%20bears%20interest%20or%20is%20a%20discounted%20government%20or%20corporate%20security%20that%20obliges%20the%20issuer%20to%20pay%20the%20bondholder%20a%20specified%20sum%20of%20money%2C%20usually%20at%20specific%20intervals%2C%20and%20to%20repay%20the%20principal%20amount%20of%20the%20loan%20at%20maturity.%20Bondholders%20have%20no%20ownership%20privileges%2C%20as%20stockholders%20do.""";
                rdfs:label "Bond" .
                
                ex:t_Established_relationship_indicator a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """An%20established%20relationship%20indicator%20is%20an%20indicator%20that%20indicates%20if%20a%20retail%20customer%20or%20SMEs%20have%20an%20established%20relationship%20with%20the%20bank.%20An%20established%20relationship%20includes%20at%20least%20one%20of%20the%20following%20elements%3A%3Cbr%3E-%20an%20active%20contractual%20relationship%20with%20the%20institution%20of%20a%20minimum%20duration""";
                rdfs:label "Established relationship indicator" .
                
                ex:t_Actuarial_gains_and_losses a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Actuarial%20gains%20and%20losses%20is%20the%20defined%20benefit%20plan%20flow%20amount%20and%20represents%20the%20changes%20in%20the%20present%20value%20of%20the%20defined%20benefit%20obligation%20resulting%20from%20the%20experience%20adjustments%20(the%20effects%20of%20differences%20between%20the%20previous%20actuarial%20assumptions%20and%20what%20has%20actually%20occurred)%2C%20and%20the%20effect%20of%20changes%20in%20actuarial%20assumptions%20(e.g.%20a%20change%20in%20mortality%20assumptions).""";
                rdfs:label "Actuarial gains and losses" .
                
                ex:t_Risk_model a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20risk%20model%20is%20a%20system%20producing%20a%20characteristic%20(typically%20a%20number%20or%20numbers)%20which%20is%20used%20in%20business%20within%20a%20limited%20period%20of%20time%20(model%20horizon)%2C%20but%20which%20cannot%20be%20directly%20observed%20in%20the%20business%20environment%20either%20at%20the%20current%20moment%20in%20time%20or%20in%20general.""";
                rdfs:label "Risk model" .
                
                ex:t_Termination_benefit a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20termination%20benefit%20is%20an%20employee%20benefit%20that%20is%20provided%20in%20exchange%20for%20the%20termination%20of%20an%20employee's%20employment%20as%20a%20result%20of%20either%3A%20%3Cbr%3E-%20an%20organisation's%20decision%20to%20terminate%20an%20employee's%20employment%20before%20the%20normal%20retirement%20date""";
                rdfs:label "Termination benefit" .
                
                ex:t_Pension_fund a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20pension%20fund%20is%20an%20investment%20fund%20that%20provides%20investment%20management%20services%20with%20the%20explicit%20aim%20of%20preservation%20and%20multiplication%20of%20capital%20for%20retirement.%20As%20a%20consequence%2C%20they%20have%20to%20be%20conservative%20in%20their%20investment%20strategy%20and%20are%20heavily%20regulated.""";
                rdfs:label "Pension fund" .
                
                ex:t_Non_risk_country a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20non-risk%20country%20is%20a%20country%20rated%20uniform%20counterparty%20rating%201.""";
                rdfs:label "Non-risk country" .
                
                ex:t_Inherent_operational_risk_exposure a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """An%20inherent%20operational%20risk%20exposure%20is%20the%20risk%20to%20an%20entity%20in%20the%20absence%20of%20any%20actions%20management%20might%20take%20to%20alter%20either%20the%20risk%D3%B3%20likelihood%20or%20impact.""";
                rdfs:label "Inherent operational risk exposure" .
                
                ex:t_Floating_interest_rate_indexed_to_market_rate a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20floating%20rate%20indexed%20to%20market%20rate%20is%20an%20interest%20rate%20that%20is%20reset%20periodically%20according%20to%20the%20fixed%20maturity%20market%20rate%20of%20a%20product%20with%20a%20duration%20extending%20beyond%20that%20of%20the%20swap's%20reset%20period.""";
                rdfs:label "Floating interest rate indexed to market rate" .
                
                ex:t_Unsubordinated_lien_position a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Unsubordinated%20lien%20position%20is%20a%20lien%20position%20that%20indicates%20a%20priority%20claim%20on%20the%20collateral%20which%20is%20established%20ahead%20of%20any%20other%20lien%20on%20the%20property.""";
                rdfs:label "Unsubordinated lien position" .
                
                ex:t_Synthetic_securitisation a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20synthetic%20securitisation%20is%20a%20securitisation%20where%20the%20transfer%20of%20risk%20is%20achieved%20by%20the%20use%20of%20credit%20derivatives%20or%20guarantees%2C%20and%20the%20exposures%20being%20securitised%20remain%20exposures%20of%20the%20originator%20institution.""";
                rdfs:label "Synthetic securitisation" .
                
                ex:t_Inventory__stock a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Inventory%20%2F%20stock%20is%20a%20tangible%20resource%20that%20is%20assets%20held%20for%20sale%20in%20the%20ordinary%20course%20of%20business%20(finished%20goods)%2C%20assets%20in%20the%20production%20process%20for%20sale%20in%20the%20ordinary%20course%20of%20business%20(work%20in%20process)%2C%20and%20materials%20and%20supplies%20that%20are%20consumed%20in%20production%20(raw%20materials).%20""";
                rdfs:label "Inventory /stock" .
                
                ex:t_Modified_duration a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """The%20modified%20duration%20is%20the%20average%20weighted%20maturity%20of%20the%20instrument%2C%20and%20a%20measure%20of%20the%20instrument%D3%B3%20price%20sensitivity%20to%20interest%20rate%20fluctuations.%20It%20is%20used%20to%20determine%20the%20risk-weighted%20position%20and%20enables%20a%20more%20risk-sensitive%20estimate%20of%20the%20interest%20rate%20risk.""";
                rdfs:label "Modified duration" .
                
                ex:t_Operating_expense_resource a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Operating%20expense%20resource%20is%20an%20expense%20resource%20that%20is%20related%20to%20the%20operating%20expenses%20of%20the%20Bank%2C%20excluding%20human%20resource.""";
                rdfs:label "Operating expense resource" .
                
                ex:t_Current_liability a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Deferred%20tax%20is%20the%20income%20taxes%20payable%20or%20receivable%20in%20future%20periods.""";
                rdfs:label "Current liability" .
                
                ex:t_FIFO_counting_method a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """TBD""";
