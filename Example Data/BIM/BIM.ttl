                ex:t_Inventory_purpose a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Inventory%20purpose%20is%20a%20counterparty%20purpose%20where%20the%20product%20is%20used%20to%20finance%20stocks%20of%20goods%20that%20are%20held%20by%20an%20involved%20party.""";
                rdfs:label "Inventory purpose" .
                
                ex:t_Financial_Statement_audit_fee a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20financial%20statement%20audit%20fee%20is%20an%20operating%20expense%20resource%20paid%20to%20the%20principal%20external%20auditor%20for%20the%20audit%20of%20the%20Financial%20statements.""";
                rdfs:label "Financial Statement audit fee" .
                
                ex:t_Number_of_accountholders a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Number%20of%20accountholders%20indicates%20whether%20an%20account%20has%20a%20single%20account%20holder%20or%20more%20than%20one%20account%20holder.""";
                rdfs:label "Number of accountholders" .
                
                ex:t_Original_fixed_interest_date a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """The%20original%20fixed%20interest%20date%20is%20the%20date%20that%20is%20chosen%20by%20the%20customer%20at%20which%20the%20fixed%20interest%20expires.%20During%20the%20original%20fixed%20interest%20period%20the%20interest%20rate%20does%20not%20fluctuate.""";
                rdfs:label "Original fixed interest date" .
                
                ex:t_Supervision_contribution a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Supervision%20contribution%20is%20an%20operating%20expense%20resource%20that%20is%20characterised%20as%20expenses%20of%20the%20supervisors%20that%20have%20to%20be%20paid%20by%20the%20Bank.""";
                rdfs:label "Supervision contribution" .
                
                ex:t_Intellectual_property a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Intellectual%20property%20is%20an%20intangible%20resource%20that%20represents%20a%20right%20to%20specific%20knowledge%20of%20an%20object%20or%20process%20such%20as%20copyright%20and%20patents.""";
                rdfs:label "Intellectual property" .
                
                ex:t_Operational_risk_Basel_business_line a dl:Data_Requirement, dl:BIM_Concept ;
