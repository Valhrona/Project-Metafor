                ex:t_Leasing_arrangement a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20leasing%20arrangement%20is%20a%20financing%20product%20arrangement%20that%20specifies%20the%20terms%20by%20which%20the%20counterparty%20obtains%20the%20use%20of%20a%20leasing%20product.%3Cbr%3E""";
