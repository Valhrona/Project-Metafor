                ex:t_Securitisation_originator a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20securitisation%20originator%20is%20the%20organisation%20which%20assets%20and%2For%20credit%20risk%20transfer%20to%20the%20Special%20Purpose%20Entity%20(SPE).""";
