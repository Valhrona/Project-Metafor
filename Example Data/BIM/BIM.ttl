                ex:t_Firm_commitment a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20firm%20commitment%20is%20a%20binding%20agreement%20for%20the%20exchange%20of%20a%20specified%20quantity%20of%20resources%20at%20a%20specified%20price%20on%20a%20specified%20future%20date%20or%20dates.""";
