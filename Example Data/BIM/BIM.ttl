                ex:t_Deposit_Inlay a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20deposit%20inlay%20is%20a%20deposit%20flow%20amount%20which%20represents%20the%20new%20amounts%20received%20in%20the%20reporting%20period%20which%20applies%20to%20existing%20or%20new%20deposit%20instruments.""";
                rdfs:label "Deposit Inlay" .
                
                ex:t_Financial_instrument_sold_at___10___discount a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Financial%20instrument%20sold%20at%20%3E%2010%20%25%20discount%20indicates%20that%20the%20Bank%20has%20sold%20a%20financial%20instrument%20issued%20by%20the%20Counterparty%20against%20a%20price%20materially%20(more%20than%2010%25)%20below%20its%20book%20value%20due%20to%20an%20increased%20credit%20risk%20related%20to%20the%20Counterparty.%20""";
