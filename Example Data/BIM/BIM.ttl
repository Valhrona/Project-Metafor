                ex:t_Gold_accounts_collateral a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Gold%20accounts%20collateral%20is%20cash%20and%20deposit%20collateral%20that%20is%20characterised%20by%20a%20current%20account%20which%20underlying%20value%20reflects%20the%20market%20value%20of%20gold.""";
                rdfs:label "Gold accounts collateral" .
                
                ex:t_The_disappearance_of_an_active_market_of_a_financial_asset a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """The%20disappearance%20of%20an%20active%20market%20of%20a%20financial%20asset%20is%20an%20active%20market%20that%20dissapeared%20for%20a%20financial%20asset%20because%20of%20financial%20difficulties.""";
                rdfs:label "The disappearance of an active market of a financial asset" .
                
                ex:t_Residual_maturity a dl:Data_Requirement, dl:BIM_Concept ;
