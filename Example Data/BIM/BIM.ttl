                rdfs:label "Credit facility arrangement" .
                
                ex:t_Basic_indicator_approach_for_operational_risk a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """The%20Basic%20Indicator%20Approach%20(BIA)%20for%20operational%20risk%20is%20an%20operational%20risk%20capital%20calculation%20which%20is%20based%20on%20a%20fixed%20percentage%20of%20the%20average%20of%20all%20operational%20risk%20gross%20income%20of%20the%20last%20three%20full%20reporting%20years.""";
                rdfs:label "Basic indicator approach for operational risk" .
                
                ex:t_Access_and_transfer_service a dl:Data_Requirement, dl:BIM_Concept ;
