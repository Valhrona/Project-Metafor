                ex:t_Involuntary_debt_restructuring a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Involuntary%20debt%20restructuring%20indicates%20that%20an%20involuntary%20restructuring%20of%20the%20facility(ies)%20granted%20to%20the%20counterparty%20is%20anticipated%20to%20take%20place%20or%20has%20taken%20place.%20This%20restructuring%20is%20to%20avoid%20a%20situation%20which%20might%20otherwise%20have%20led%20to%20a%20payment%20default.""";
                rdfs:label "Involuntary debt restructuring" .
                
                ex:t_Eligible_double_default_guarantor a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """An%20eligible%20double%20default%20guarantor%20is%20a%20guarantor%20that%20meets%20eligibility%20conditions%20in%20order%20for%20the%20double%20default%20formula%20to%20be%20applicable.%20For%20regulatory%20capital%20(RC)%20purposes%2C%20the%20double%20default%20formula%20is%20applied%20only%20if%20one%20of%20the%20following%20conditions%20is%20met%3A%3Cbr%3E-%20the%20guarantor%20is%20a%20bank%20or%20(re-)insurance%20company""";
