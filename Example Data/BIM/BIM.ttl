                ex:t_Arrangement_term_type a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """An%20arrangement%20term%20type%20is%20a%20classification%20scheme%20that%20distinguishes%20between%20arrangements%20according%20to%20whether%20the%20arrangement%20is%20for%20a%20fixed%20or%20a%20variable%20tenor.""";
