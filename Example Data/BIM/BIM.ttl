                ex:t_Reporting_entity a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20reporting%20entity%20is%20an%20entity%2C%20a%20group%20of%20entities%20or%20a%20part%20of%20an%20entity%20of%20the%20bank%20that%20is%20required%20to%20submit%20its%20financial%20data%20for%20consolidation%20purposes.""";
