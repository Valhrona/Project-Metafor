                rdfs:label "First loss tranche" .
                
                ex:t_Pensioner a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """No%20definition%20available%3A%20No%20clear%20guidelines%20received%20from%20ECB.""";
                rdfs:label "Pensioner" .
                
                ex:t_Date_of_debt_restructuring a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20date%20of%20debt%20restructuring%20is%20the%20date%20at%20which%20the%20first%20restructuring%20measures%20commence.""";
