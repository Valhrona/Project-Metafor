                ex:t_Current_tax_offsetting a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Current%20tax%20offsetting%20is%20tax%20offsetting%20for%20current%20tax.%20""";
                rdfs:label "Current tax offsetting" .
                
                ex:t_Guarantor_identifier a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Guarantor%20identifier%20is%20a%20unique%20code%20to%20identify%20the%20guarantor%20given%20by%20the%20National%20Central%20Credit%20Register%20of%20the%20guarantor%20and%20using%20LEI%20%2F%20preLEI%20for%20large%20non%20Euro%20area%20entities.""";
