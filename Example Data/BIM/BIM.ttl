                ex:t_Date_of_birth a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """%3Cp%3EDate%20of%20birth%20is%20the%20date%20at%20which%20the%20private%20individual%20was%20born.%3Cbr%3E%3C%2Fp%3E""";
                rdfs:label "Date of birth" .
                
                ex:t_Currency a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20currency%20is%20a%20unit%20of%20measure%20used%20for%20money%2C%20including%20coins%20and%20paper%20notes.""";
                rdfs:label "Currency" .
                
                ex:t_Change_risk a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Change%20risk%20is%20the%20risk%20associated%20with%20the%20development%20and%20implementation%20of%20new%20or%20changed%20products%2C%20activities%2C%20processes%2C%20systems%20or%20organisation%2C%20e.g.%20changes%20in%20the%20IT%20infrastructure%20or%20an%20acquisition.""";
                rdfs:label "Change risk" .
                
                ex:t_Financial_market_instrument_flow_amount_indicator a dl:Data_Requirement, dl:BIM_Concept ;
