                ex:t_Retail_asset_class a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20retail%20asset%20class%20is%20a%20categorisation%20under%20the%20internal%20rating-based%20approach%2C%20that%20encompasses%20the%20following%20characteristics%2C%20it%20shall%3A%20%3Cbr%3E-%20be%20either%20to%20one%20or%20more%20natural%20persons%20or%20to%20a%20small%20or%20medium-sized%20entity""";
