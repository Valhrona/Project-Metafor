                ex:t_Prudential_filter a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20prudential%20filter%20apply%20to%20correct%20for%20components%20that%20regulators%20do%20not%20consider%20sufficiently%20loss%20absorbing.%20""";
                rdfs:label "Prudential filter" .
                
                ex:t_Uniform_Counterparty_Rating a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20uniform%20counterparty%20rating%20is%20the%20result%20of%20an%20assessment%20by%20the%20bank%20of%20the%20probability%20of%20default%20of%20the%20counterparty%20with%20respect%20to%20its%20financial%20obligations%20to%20the%20bank%20within%20the%20next%20twelve%20months.%20This%20assessment%20is%20expressed%20as%20a%20percentage%2C%20the%20PD%2C%20and%20translated%20into%20an%20internal%20rating%20referred%20to%20as%20uniform%20counterparty%20rating%20or%20%D4%95CR%D4%AE""";
                rdfs:label "Uniform Counterparty Rating" .
                
                ex:t_Interest_payment_frequency a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """The%20interest%20payment%20frequency%20is%20the%20frequency%20of%20the%20interest%20payments%20agreed%20with%20the%20counterparty%20on%20a%20contractual%20basis.""";
                rdfs:label "Interest payment frequency" .
                
                ex:t_Assets_under_Management a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Assets%20under%20Management%20(AuM)%20are%20assets%2C%20including%20investment%20funds%20and%20assets%20of%20private%20individuals%20and%20institutions%2C%20that%20are%20professionally%20managed%20with%20the%20aim%20to%20maximize%20the%20investment%20result.""";
                rdfs:label "Assets under Management" .
                
                ex:t_Risk_weight a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20risk%20weight%20is%20a%20measure%20that%20is%20used%20to%20assign%20a%20weight%20that%20reflects%20the%20relative%20riskiness%20of%20an%20exposure%20in%20order%20to%20derive%20the%20capital%20requirement.""";
                rdfs:label "Risk weight" .
                
                ex:t_Forborne_status_date a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Is%20the%20date%20that%20determines%20whether%20the%20arrangement%20...""";
