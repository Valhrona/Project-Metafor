                dl:definition """A%20multilateral%20organisations%20non%20bank%20is%20an%20organisation%20that%20is%20established%20by%20multiple%20countries%20with%20the%20purpose%20of%20international%20cooperation%20and%20stabilisation%2C%20e.g.%20EBRD%2C%20EFSF%20and%20ESM.""";
                rdfs:label "Multilateral organisations non bank" .
                
                ex:t_Settlement_risk a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Settlement%20risk%20is%20the%20risk%20that%20the%20counterparty%20defaults%20on%20transactions%20in%20the%20process%20of%20being%20settled%2C%20where%20value%20has%20been%20delivered%20to%20the%20counterparty%20but%20not%20yet%20received%20in%20return.""";
                rdfs:label "Settlement risk" .
                
                ex:t_Captive_financial_institutions_and_money_lenders a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20captive%20financial%20institution%20and%20money%20lender%20is%20a%20financial%20institution%20or%20organisation%20where%20most%20of%20either%20their%20assets%20or%20their%20liabilities%20are%20not%20transacted%20on%20open%20markets%20and%20which%20are%20neither%20engaged%20in%20financial%20intermediation%20nor%20in%20providing%20financial%20auxiliary%20services.""";
                rdfs:label "Captive financial institutions and money lenders" .
                
                ex:t_Preference_share a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20preference%20share%20is%20an%20equity%20instrument%20that%20carries%20rights%20to%20a%20fixed%20amount%20of%20dividend%20and%20return%20of%20capital%20in%20priority%20to%20ordinary%20shares.""";
                rdfs:label "Preference share" .
                
                ex:t_Commercial_real_estate_collateral a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Commercial%20real%20estate%20collateral%20is%20real%20estate%20collateral%20such%20as%20land%20and%20anything%20permanently%20fixed%20thereto%2C%20as%20well%20as%20any%20rights%20or%20interest%20in%20land%20owned%20by%20project%20developers%20or%20investors%20with%20the%20purpose%20to%20develop%2C%20trade%20or%20rent%20the%20land%20or%20property%2C%20for%20the%20longer%20term.%20""";
                rdfs:label "Commercial real estate collateral" .
                
                ex:t_Foreclosed_asset a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20foreclosed%20asset%20is%20a%20feature%20of%20a%20tangible%20asset%20obtained%20by%20taking%20possession%20of%20collateral%20that%20remains%20recognised%20in%20the%20balance%20sheet%20at%20the%20reference%20date.""";
                rdfs:label "Foreclosed asset" .
                
                ex:t_Maximum_consumer_obligo a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """The%20maximum%20consumer%20obligo%20is%20the%20maximum%20amount%20a%20private%20individual%20can%20borrow%20based%20on%20his%20income%2C%20obligations%20and%20a%20conversion%20factor.""";
                rdfs:label "Maximum consumer obligo" .
                
                ex:t_Variable_term a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20variable%20term%20is%20a%20measurement%20that%20indicates%20that%20the%20tenor%20of%20an%20arrangement%20or%20product%20is%20variable.%3Cbr%3E""";
                rdfs:label "Variable term" .
                
                ex:t_French_amortisation a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20French%20amortisation%20is%20an%20amortisation%20type%20in%20which%20a%20constant%20instalment%20is%20calculated%20throughout%20the%20life%20of%20the%20loan%20with%20the%20principal%20component%20increasing%20over%20time%20and%20the%20interest%20element%20decreasing%20over%20time.""";
                rdfs:label "French amortisation" .
                
                ex:t_Book_value a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Book%20value%20is%20an%20amount%20indicating%20a%20resource%20item%20value%20that%20reflects%20the%20original%20price%20paid%20for%20the%20resource%20item%20minus%20its%20accumulated%20depreciation%20to%20date.%20This%20is%20considered%20the%20official%20value%20of%20a%20resource%20item%20and%20it%20is%20usually%20the%20value%20carried%20on%20the%20balance%20sheet.%20""";
                rdfs:label "Book value" .
                
                ex:t_Security_transaction_service a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Security%20transaction%20service%20is%20a%20securities%20service%20that%20includes%20the%20execution%20of%20orders%20to%20buy%20or%20sell%20securities%20on%20behalf%20of%20customers.""";
                rdfs:label "Security transaction service" .
                
                ex:t_No_recourse a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """No%20recourse%20is%20recourse%20that%20indicates%20the%20absence%20of%20the%20Bank's%20right%20to%20take%20any%20asset%20of%20the%20borrower%20other%20than%20the%20collateral.""";
                rdfs:label "No recourse" .
                
                ex:t_Unrealised_result_securities a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """An%20unrealised%20result%20securities%20is%20the%20financial%20market%20instrument%20flow%20amount%20representing%20the%20unrealised%20result%20during%20a%20reporting%20period%20of%20a%20security%20instrument%20in%20an%20open%20position.""";
                rdfs:label "Unrealised result securities" .
                
                ex:t_Terminated_arrangement a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20terminated%20arrangement%20is%20the%20status%20of%20an%20arrangement%20which%20signifies%20that%20the%20obligations%20have%20not%20been%20fully%20met%20within%20the%20period%20of%20the%20arrangement%20and%20in%20which%20one%20party%20to%20the%20arrangement%20has%20ended%20the%20arrangement%20prematurely.""";
                rdfs:label "Terminated arrangement" .
                
                ex:t_Restitution a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20restitution%20is%20a%20compensation%20payment%20to%20counterparties%20on%20account%20of%20operational%20risk%20events.""";
                rdfs:label "Restitution" .
                
                ex:t_Client_group_code a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Client%20group%20code%20(CGC)%20is%20the%20segmentation%20code%20for%20clients%20in%20a%20group%20hierarchy%20based%20on%20expected%20potential%20and%20servicing%20units.""";
                rdfs:label "Client group code" .
                
                ex:t_Defined_benefit_plan a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20defined%20benefit%20plan%20is%20a%20post-employment%20benefit%20plan%20in%20which%20the%20organisation%20has%20an%20obligation%20to%20provide%20agreed%20benefits%20to%20current%20and%20former%20employees.%20In%20a%20defined%20benefit%20scheme%20both%20the%20actuarial%20and%20investment%20risk%20falls%20on%20the%20organisation.""";
                rdfs:label "Defined benefit plan" .
                
                ex:t_Payment_collection_service a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Payment%20collection%20service%20is%20an%20access%20and%20transfer%20service%20which%20refers%20to%20the%20collection%20on%20behalf%20of%20customers%20of%20payments%20generated%20by%20debt%20instruments%20that%20are%20neither%20recognised%20on%20the%20balance%20sheet%20of%20the%20bank%20nor%20originated%20by%20it.""";
                rdfs:label "Payment collection service" .
                
                ex:t_Effective_maturity_date a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """The%20effective%20maturity%20date%20is%20the%20expected%20end%20date%20of%20the%20arrangement.""";
                rdfs:label "Effective maturity date" .
                
                ex:t_At_cost a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """At%20cost%20is%20a%20valuation%20method.%20'At%20cost'%20can%20mean%20at%20'amortised%20cost'%20or%20at%20'historical%20cost'.%20Financial%20assets%20whose%20fair%20value%20cannot%20be%20reliably%20measured%20(limited%20to%20some%20equity%20securities%20with%20no%20quoted%20market%20price%20and%20derivatives%20such%20as%20forwards%20and%20options%20on%20unquoted%20equity%20securities)%20shall%20be%20measured%20at%20cost.%20Financial%20instruments%20for%20which%20fair%20value%20can%20be%20reliably%20measured%20can%20still%20be%20measured%20at%20amortised%20cost.""";
                rdfs:label "At cost" .
                
                ex:t_Currency_translation_reserve a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """The%20currency%20translation%20reserve%20is%20a%20capital%20resource%20that%20represents%20the%20cumulative%20gains%20and%20losses%20on%20the%20translation%20of%20the%20net%20investment%20in%20foreign%20operations%2C%20net%20of%20the%20effect%20of%20hedging.""";
                rdfs:label "Currency translation reserve" .
                
                ex:t_Project_finance_loan a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Project%20financing%20is%20a%20financing%20structure%20that%20relies%20primarily%20on%20the%20project's%20cash%20flow%20for%20the%20payment%20of%20interest%20and%20repayment%20of%20principal%2C%20with%20the%20project's%20assets%2C%20rights%2C%20and%20interests%20held%20as%20security%20or%20collateral.""";
                rdfs:label "Project finance loan" .
                
                ex:t_Supranational_government a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20supranational%20government%20is%20a%20government%20in%20which%20multiple%20countries%20participate%20and%20objectives%20go%20beyond%20national%20power%2C%20interest%20and%20borders.""";
                rdfs:label "Supranational government" .
                
                ex:t_Undrawn_amount a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """An%20undrawn%20amount%20is%20an%20amount%20which%20denotes%20the%20difference%20between%20the%20overdraft%20or%20credit%20limit%20amount%20and%20the%20current%20outstanding%20balance.""";
                rdfs:label "Undrawn amount" .
                
                ex:t_Limit_type a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Limit%20type%20is%20a%20classification%20scheme%20that%20distinguishes%20between%20limit%20conditions%20according%20to%20the%20object%20of%20the%20limit.%20The%20values%20defined%20for%20this%20scheme%20are%20representative%20and%20do%20not%20comprise%20an%20exhaustive%20list.""";
                rdfs:label "Limit type" .
                
                ex:t_Money_market_fund a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20money%20market%20fund%20is%20an%20investment%20fund%20with%20the%20following%20characteristics%3A%3Cbr%3E1.%20In%20terms%20of%20liquidity%2C%20the%20stocks%20%2F%20participations%20are%20close%20substitutes%20for%20deposits%20and%20which%20invest%20more%20than%2085%25%20of%20the%20investment%20portfolio%20in%20money%20market%20instruments%20and%20%2F%20or%20in%20stocks%20%2F%20participations%20of%20money%20market%20funds%20and%20%2F%20or%20in%20other%20transferable%20debt%20instruments%20with%20a%20remaining%20maturity%20up%20to%20one%20year%20and%20%2F%20or%20in%20bank%20deposits.%3Cbr%3E2.%20Aim%20for%20a%20return%20that%20levels%20the%20interest%20return%20on%20money%20market%20instruments.""";
                rdfs:label "Money market fund" .
                
                ex:t_Indirect_recovery_payment a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """An%20indirect%20recovery%20payment%20is%20a%20payment%20or%20repayment%20from%20an%20internal%20or%20external%20insurer%20that%20reduces%20the%20financial%20impact%20of%20the%20operational%20risk%20loss.""";
                rdfs:label "Indirect recovery payment" .
                
                ex:t_Loan_to_value_ratio a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Loan%20to%20Value%20(LTV)%20ratio%20is%20a%20measure%20that%20expresses%20the%20amount%20of%20a%20loan%20as%20a%20percentage%20of%20the%20total%20appraised%20value%20of%20collateral.""";
                rdfs:label "Loan to value ratio" .
                
                ex:t_Vehicle_collateral a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Vehicle%20collateral%26nbsp""";
                rdfs:label "Vehicle collateral" .
                
                ex:t_Reporting_risk a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Reporting%20risk%20is%20the%20risk%20of%20misstatements%20in%20reported%20information%20-%20incorrect%2C%20incomplete%2C%20etc.%20-%20to%20internal%20parties%2C%20the%20general%20public%2C%20authorities%20or%20regulators%20and%20the%20risk%20that%20to%20be%20reported%20earnings%20or%20to%20be%20repored%20earnings%2C%20valuations%20and%20cash%20flows%20of%20the%20bank%20change%20without%20a%20change%20in%20economic%20value%20of%20the%20bank.""";
                rdfs:label "Reporting risk" .
                
                ex:t_Tranche_type a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20tranche%20type%20is%20a%20classification%20scheme%20for%20asset-backed%20securities%20that%20is%20characterised%20as%20a%20contractually%20established%20segment%20of%20the%20credit%20risk%20associated%20with%20an%20exposure%20or%20a%20number%20of%20exposures.%20In%20it%20a%20position%20in%20the%20segment%20entails%20a%20risk%20of%20credit%20loss%20greater%20than%20or%20less%20than%20a%20position%20of%20the%20same%20amount%20in%20each%20other%20such%20segment.%20It%20does%20not%20take%20account%20of%20credit%20protection%20provided%20by%20third%20parties%20directly%20to%20the%20holders%20of%20positions%20in%20the%20segment%20or%20in%20other%20segments.""";
                rdfs:label "Tranche type" .
                
                ex:t_Termination_of_the_credit_facility_arrangement a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Termination%20of%20the%20credit%20facility%20arrangement%20is%20a%20classification%20scheme%20that%20distinguishes%20between%20credit%20facilities%20that%20are%20terminated%20by%20repayment%2C%20by%20cancellation%20by%20the%20counterparty%2C%20i.e.%20unused%20facilities%2C%20or%20by%20termination%20by%20The%20Bank.""";
                rdfs:label "Termination of the credit facility arrangement" .
                
                ex:t_Unfunded_Credit_Protection a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Unfunded%20credit%20protection%20is%20a%20technique%20of%20credit%20risk%20mitigation%20where%20the%20reduction%20of%20the%20credit%20risk%20on%20the%20exposure%20of%20an%20institution%20derives%20from%20the%20obligation%20of%20a%20third%20party%20to%20pay%20an%20amount%20in%20the%20event%20of%20the%20default%20of%20the%20borrower%20or%20the%20occurrence%20of%20other%20specified%20credit%20events.""";
                rdfs:label "Unfunded Credit Protection" .
                
                ex:t_Deferred_tax_asset_write_down a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Deferred%20tax%20asset%20write%20down%20is%20a%20write%20down%20of%20a%20deferred%20tax%20asset.""";
                rdfs:label "Deferred tax asset write down" .
                
                ex:t_Accounting_mismatch_risk a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Accounting%20mismatch%20risk%20is%20the%20risk%20that%20reported%20earnings%20of%20the%20bank%20change%20without%20a%20change%20in%20economic%20value%20of%20the%20bank.""";
                rdfs:label "Accounting mismatch risk" .
                
                ex:t_Salaries_and_wages a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Salaries%20and%20wages%20is%20a%20short-term%20employment%20benefit%20paid%20to%20employees%20of%20the%20Bank%20for%20the%20services%20that%20they%20render.""";
                rdfs:label "Salaries and wages" .
                
                ex:t_Past_due_start_date a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """TBD""";
                rdfs:label "Past due start date" .
                
                ex:t_Resource_item_flow_amount_indicator a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Resource%20item%20flow%20amount%20indicator%20is%20an%20indicator%20that%20classifies%20the%20resource%20item%20flow%20amount.""";
                rdfs:label "Resource item flow amount indicator" .
                
                ex:t_Associate a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """An%20associate%20is%20an%20organisation%20in%20which%20the%20Bank%20has%20significant%20influence.%20""";
                rdfs:label "Associate" .
                
                ex:t_Short_position a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20short%20position%20is%20a%20position%20in%20a%20financial%20market%20instrument%20that%20arises%20from%20the%20sale%20of%20a%20financial%20market%20instrument%20not%20owned%2C%20in%20anticipation%20of%20making%20a%20profit%20by%20purchasing%20the%20financial%20market%20instrument%20later%20at%20a%20lower%20price.""";
                rdfs:label "Short position" .
                
                ex:t_Contract_modification_in_context_of_forbearance a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20contract%20modification%20in%20the%20context%20of%20forbearance%20is%20a%20modification%20of%20the%20contract%26nbsp""";
                rdfs:label "Contract modification in context of forbearance" .
                
                ex:t_Finance___Risk_statement a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20finance%20%26%20risk%20statement%20is%20a%20cluster%20of%20statements%20containing%20the%20reported%20terms%20of%20the%20statement%20of%20financial%20position%20(balance%20sheet)%2C%20the%20statement%20of%20profit%20or%20loss%20and%20a%20limited%20list%20of%20risk%20reporting%20terms%20based%20on%20the%20Regulatory%20Capital%20Framework%20and%20the%20Economic%20Capital%20concept.""";
                rdfs:label "Finance & Risk statement" .
                
                ex:t_Multiplication_factor a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20multiplication%20factor%20is%20a%20factor%20to%20multiply%20the%20Value%20at%20Risk%20which%20is%20used%20for%20capital%20requirements%20and%20should%20be%20at%20least%203%20and%20be%20increased%20by%20an%20addend%20between%200%20and%201%20depending%20on%20the%20VaR%20backtesting%20results.""";
                rdfs:label "Multiplication factor" .
                
                ex:t_Local_Government a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20local%20government%20is%20an%20organisation%20which%20in%20a%20majority%20of%20contexts%2C%20exists%20as%20the%20lowest%20tier%20of%20administration%20within%20a%20given%20state.""";
                rdfs:label "Local Government" .
                
                ex:t_Personal_guarantor a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Personal%20guarantor%20is%20a%20guarantor%20that%20is%20a%20private%20individual.""";
                rdfs:label "Personal guarantor" .
                
                ex:t_Collateral_provided a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Collateral%20provided%20is%20collateral%20provided%20by%20the%20Bank.""";
                rdfs:label "Collateral provided" .
                
                ex:t_Provision_resource a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Provision%20resource%20is%20a%20monetary%20resource%20that%20represents%20a%20provision.""";
                rdfs:label "Provision resource" .
                
                ex:t_Credit_risk_rating a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20credit%20risk%20rating%20is%20an%20assessment%20of%20the%20probability%20of%20default%20of%20the%20counterparty%20with%20respect%20to%20its%20financial%20obligations%20within%20the%20next%20twelve%20months.""";
                rdfs:label "Credit risk rating" .
                
                ex:t_Unlikely_to_pay a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Unlikely%20to%20pay%20is%20a%20default%20trigger%20which%20indicates%20that%20the%20counterparty%20is%20unlikely%20to%20pay%20its%20financial%20obligations%20to%20the%20Bank%20in%20full%2C%20without%20recourse%20by%20the%20Bank%20to%20actions%20such%20as%20realising%20security%20if%20held.""";
                rdfs:label "Unlikely to pay" .
                
                ex:t_Administration_service a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Administration%20service%20is%20a%20service%20in%20which%20a%20counterparty%20pays%20a%20fee%20for%20the%20bank%20to%20administer%20and%20account%20for%20the%20counterparty.""";
                rdfs:label "Administration service" .
                
                ex:t_Total_capital a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Total%20capital%20is%20a%20measure%20that%20contains%20the%20capital%20buffer%2C%20tier%201%20capital%2C%20and%20tier%202%20capital.""";
                rdfs:label "Total capital" .
                
                ex:t_Gold___Underlying_asset a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Gold%20-%20Underlying%20asset%20is%20an%20underlying%20asset%20that%20is%20based%20on%20gold.""";
                rdfs:label "Gold - Underlying asset" .
                
                ex:t_Resultant_exposure_class a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Resultant%20exposure%20class%20is%20the%20exposure%20class%20after%20substitution%20of%20the%20guarantees.""";
                rdfs:label "Resultant exposure class" .
                
                ex:t_Controlled_early_amortisation_securitisation a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Controlled%20early%20amortisation%20securitisation%26nbsp""";
                rdfs:label "Controlled early amortisation securitisation" .
                
                ex:t_Netting a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Netting%20is%20the%20merging%20of%20two%20or%20more%20positions%20in%20order%20to%20present%20a%20single%20position.""";
                rdfs:label "Netting" .
                
                ex:t_Software a dl:Data_Requirement, dl:BIM_Concept ;
