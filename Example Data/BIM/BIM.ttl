                ex:t_Incremental_Risk_Charge a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """The%20Incremental%20Risk%20Charge%20represents%20an%20estimate%20of%20the%20default%20and%20migration%20risks%20of%20unsecuritised%20credit%20products%20over%20a%20one-year%20capital%20horizon%20at%20a%2099.9%20percent%20confidence%20level%2C%20taking%20into%20account%20the%20liquidity%20horizons%20of%20individual%20positions%20or%20sets%20of%20positions.""";
