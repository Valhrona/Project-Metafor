                rdfs:label "Purchase of equity or financial market instruments" .
                
                ex:t_Revaluation_frequency_for_security_positions a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """The%20revaluation%20frequency%20is%20the%20number%20of%20business%20days%20between%20revaluations%20of%20the%20drawing.""";
