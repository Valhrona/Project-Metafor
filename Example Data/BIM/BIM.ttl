                dl:definition """Sustainability%20risk%20is%20the%20risk%20of%20practices%20by%20the%20bank%2C%20its%20business%20relations%20and%20service%20providers%2C%20damaging%20the%20sustainability%20profile%20of%20the%20bank%20in%20the%20perception%20of%20the%20bank's%20stakeholders.""";
                rdfs:label "Sustainability risk" .
                
                ex:t_Standard___Poor_s_credit_rating a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """The%20Standard%20%26%20Poor's%20credit%20rating%20expresses%20the%20agency%20opinion%20about%20the%20likelihood%20(willingness%20and%20ability)%20of%20an%20issuer%20to%20meet%20its%20financial%20obligation%20in%20full%20and%20on%20time.%3Cbr%3E""";
                rdfs:label "Standard & Poor's credit rating" .
                
                ex:t_Step_in_Declaration_II a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20step-in%20declaration%20II%20is%20a%20kind%20of%20confirmative%20declaration%20issued%20by%20a%20group%20company%20to%20a%20counterparty%26nbsp""";
                rdfs:label "Step-in Declaration II" .
                
                ex:t_Accrued_interest_receivable a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Accrued%20interest%20receivable%20is%20the%20accrued%20interest%20amount%20that%20is%20recognised%20and%20is%20not%20yet%20received.""";
