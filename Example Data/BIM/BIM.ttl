                rdfs:label "Cash and deposits collateral" .
                
                ex:t_Organisation_unit_authority_designation a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """An%20organisation%20unit%20authority%20designation%20is%20a%20classification%20scheme%20that%20distinguishes%20between%20organisation%20units%20based%20on%20the%20scope%20or%20sphere%20of%20their%20authority%20or%20responsibility.""";
                rdfs:label "Organisation unit authority designation" .
                
                ex:t_Economic_profit a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Economic%20profit%20expresses%20the%20risk%20adjusted%20return%20as%20an%20amount%20after%20deduction%20of%20the%20cost%20of%20economic%20capital.""";
