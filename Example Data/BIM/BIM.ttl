                ex:t_Residential_real_estate_collateral a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Residential%20real%20estate%20collateral%20is%20real%20estate%20that%20is%2C%20or%20will%20be%2C%20occupied%20as%20a%20primary%20or%20secondary%20residence%20by%20the%20private%20owner%20of%20the%20beneficial%20owner%20in%20case%20of%20personal%20investment%20companies.""";
                rdfs:label "Residential real estate collateral" .
                
                ex:t_Macro_hedge a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20macro%20hedge%20is%20a%20hedge%20used%20to%20eliminate%20the%20risk%20of%20a%20portfolio%20of%20assets%20or%20liabilities.%20""";
                rdfs:label "Macro hedge" .
                
                ex:t_Market_risk a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Market%20risk%20is%20the%20risk%20that%20the%20value%20and%2For%20the%20earnings%20of%20the%20bank%20decline%20because%20of%20unfavourable%20market%20movements.""";
                rdfs:label "Market risk" .
                
                ex:t_Cash_and_deposits_collateral a dl:Data_Requirement, dl:BIM_Concept ;
