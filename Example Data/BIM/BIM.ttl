                ex:t_Amortisation_charge a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Amortisation%20charge%20classifies%20an%20amount%20that%20represents%20the%20charge%20due%20to%20amortisation.""";
                rdfs:label "Amortisation charge" .
                
                ex:t_Source_of_encumbrance a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Source%20of%20encumbrance%20is%20the%20link%20between%20the%20transactions%20which%20are%20collateralised%20by%20the%20bank%2C%26nbsp""";
                rdfs:label "Source of encumbrance" .
                
                ex:t_Loan_equivalent_factor a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """The%20loan%20equivalent%20factor%20expresses%20the%20likelihood%20of%20conversion%20of%20a%20guarantee%20into%20a%20cash%20exposure%20before%20default%20is%20called.""";
                rdfs:label "Loan equivalent factor" .
                
                ex:t_Purchase_of_equity_or_financial_market_instruments a dl:Data_Requirement, dl:BIM_Concept ;
