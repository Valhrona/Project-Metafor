                ex:t_Valuation_date a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """Valuation%20date%20is%20the%20date%20upon%20which%20the%20asset%2C%20collateral%20and%20resource%20item%20were%20valued%20according%20to%20a%20specific%20valuation%20method.""";
                rdfs:label "Valuation date" .
                
                ex:t_Special_purpose_entity_engaged_in_securitisation_transactions a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20Special%20purpose%20entity%20is%20an%20institutional%20sector%20that%20intends%20to%20carry%20out%2Cone%20or%20more%20securitisation%20transactions%20and%20its%20structure%20is%20intended%20to%20isolate%20the%20payment%20obligations%20of%20the%20undertaking%20from%20those%20of%20the%20originator%20and%20it%20issues%20debt%20securities%2C%20other%20debt%20instruments%2C%20securitisation%20fund%20units%2C%20and%2For%20financial%20derivatives%20and%2For%20legally%20or%20economically%20owns%2C%20or%20may%20own%2C%20assets%20underlying%20the%20issue%20of%20financing%20instruments%20that%20are%20offered%20for%20sale%20to%20the%20public%20or%20sold%20on%20the%20basis%20of%20private%20placements.""";
