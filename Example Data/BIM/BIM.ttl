                ex:t_Deferred_tax_asset a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20deferred%20tax%20asset%20is%20an%20asset%20that%20represents%20the%20amount%20of%20income%20taxes%20recoverable%20in%20future%20periods%20in%20respect%20of%3A%3Cbr%3Ea.%20tax%20deductible%20temporary%20differences%3Cbr%3Eb.%20the%20carry%20forward%20of%20unused%20tax%20losses%2C%20and%3Cbr%3Ec.%20the%20carry%20forward%20of%20unused%20tax%20credits.%3Cbr%3E""";
                rdfs:label "Deferred tax asset" .
                
                ex:t_Repurchase_agreement a dl:Data_Requirement, dl:BIM_Concept ;
                dl:definition """A%20repurchase%20agreement%20is%20security%20financing%20that%20the%20simultaneous%20sale%20of%20securities%20together%20with%20an%20agreement%20for%20the%20seller%20to%20buy%20back%20(%D2%B2epurchase%D2%A9%20the%20securities%20at%20a%20later%20date.%20The%20repurchase%20price%20is%20typically%20greater%20than%20the%20original%20sale%20price%2C%20the%20difference%20representing%20the%20repo%20rate.""";
